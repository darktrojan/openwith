{
	"manifest_version": 2,
	"name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
	"version": "7.0.3",
	"default_locale": "en",
	"applications": {
		"gecko": {
			"id": "openwith@darktrojan.net",
			"strict_min_version": "55.0"
		}
	},
	"icons": {
		"16": "images/openwith16.png",
		"24": "images/openwith24.png",
		"32": "images/openwith32.png",
		"48": "images/openwith48.png",
		"64": "images/openwith64.png",
		"96": "images/openwith96.png"
	},
	"background": {
		"scripts": [
			"common.js",
			"background.js"
		]
	},
	"permissions": [
		"contextMenus",
		"nativeMessaging",
		"storage",
		"tabs"
	],
	"browser_action": {
		"default_icon": {
			"16": "images/openwith16.png",
			"24": "images/openwith24.png",
			"32": "images/openwith32.png",
			"48": "images/openwith48.png",
			"64": "images/openwith64.png",
			"96": "images/openwith96.png"
		},
		"default_popup": "action.html",
		"browser_style": false
	},
	"options_ui": {
		"browser_style": false,
		"open_in_tab": true,
		"page": "options.html"
	},
        "commands": {
                "run-default": {
                      "suggested_key": {
                            "default": "Alt+Shift+P"
                      },
                      "description": "run the default browser"
                }
        }
}
